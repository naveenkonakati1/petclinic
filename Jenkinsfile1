pipeline {
   agent any
     stages{
           	stage('Checkout') {
			steps {
			git url: 'https://github.com/naveenkonakati1/petclinic.git'
			}
		}
		stage('build') {
			steps {
			   	 sh 'clean package'
   			}
	   	}
		stage('archive') {
			steps {
		    		archiveArtifacts artifacts: 'target/*.war'
		    	}
		}
		stage('unit tests') {
			steps {
		    		junit 'target/surefire-reports/*.xml'
		  	}
		}
	
            }
}
